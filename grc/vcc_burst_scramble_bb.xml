<?xml version="1.0"?>
<block>
  <name>Burst Scrambler</name>
  <key>vcc_burst_scramble_bb</key>
  <category>[vcc]</category>
  <import>import vcc</import>
  <make>vcc.burst_scramble_bb($mask, $seed, $len)</make>
  <param>
    <name>Mask</name>
    <key>mask</key>
    <value></value>
    <type>int</type>
  </param>
  <param>
    <name>Seed</name>
    <key>seed</key>
    <value></value>
    <type>int</type>
  </param>
  <param>
    <name>Length</name>
    <key>len</key>
    <value></value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
